<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<fx:root type="FlowPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <TextField fx:id="username" promptText="email">
        <FlowPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </FlowPane.margin>
    </TextField>
    <PasswordField fx:id="password" promptText="Password">
        <FlowPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </FlowPane.margin>
    </PasswordField>
    <Button fx:id="login" mnemonicParsing="false" text="Sign in">
        <FlowPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </FlowPane.margin>
    </Button>
</fx:root>